# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := micromouse

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

micromouse: 
	@echo "==== Building micromouse ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f micromouse.make

clean:
	@${MAKE} --no-print-directory -C . -f micromouse.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   micromouse"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
